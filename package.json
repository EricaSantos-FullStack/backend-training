{
  "name": "backend-training",
  "version": "1.0.0",
  "description": "Backend Training",
  "main": "index.js",
  "author": "Erica Francisco",
  "license": "MIT",
  "private": true,
  "jest": {},
  "scripts": {
    "dev": "nodemon --exec babel-node server/index.js",
    "clean": "rm -rf build && mkdir build",
    "build:docker": "sh build-zip.sh",
    "build:server": "npm run clean && babel -d ./build ./server -s",
    "build": "npm run build:server && npm run copy:seeders && npm run build:docker",
    "copy:seeders": "cp -r ./server/infra/seeders ./build/seeders",
    "start": "node ./build/index.js",
    "debug": "node --debug ./build/index.js",
    "test": "jest --verbose --forceExit --detectOpenHandles --maxWorkers=1",
    "migration:create": "typeorm migration:create --dir server/migrations/default --name",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate --name",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration-seeker:create": "typeorm migration:create --dir server/migrations/seeker --name",
    "migration-seeker:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -c seeker --name",
    "migration-seeker:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -c seeker",
    "migration-seeker:run": "ts-node ./node_modules/typeorm/cli.js migration:run -c seeker",
    "migration:dev": "nodemon --exec babel-node server/scripts/server.js",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@fast-csv/format": "^4.3.1",
    "@fast-csv/parse": "^4.3.2",
    "@sendgrid/mail": "^6.2.1",
    "adm-zip": "^0.4.13",
    "analytics-node": "^3.3.0",
    "apollo-server-express": "^1.3.2",
    "apollo-upload-server": "^7.0.0",
    "async": "^2.6.2",
    "aws-sdk": "^2.771.0",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "cpf": "^2.0.1",
    "dotenv": "^10.0.0",
    "expo-server-sdk": "^2.4.0",
    "express": "^4.16.2",
    "express-device": "^0.4.2",
    "fast-csv": "4.3.1",
    "firebase-admin": "^8.12.1",
    "ftp": "^0.3.10",
    "googleapis": "^37.2.0",
    "gql-merge": "^0.0.6",
    "graph-tools": "^0.1.2",
    "graphql": "^0.13.1",
    "graphql-date": "^1.0.3",
    "graphql-disable-introspection": "^1.1.0",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-tools": "^2.21.0",
    "helmet": "^3.21.2",
    "intercom-client": "^2.10.4",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.1",
    "mongoose": "^5.0.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.7.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.0.0",
    "pg": "^8.4.1",
    "sequelize": "^4.38.0",
    "supertest": "^6.2.1",
    "typeorm": "0.2.28",
    "typeorm-naming-strategies": "^1.3.0",
